{
  "version": 2,
  "builds": [
    {
      "src": "backend/api.py",
      "use": "@vercel/python"
    },
    {
      "src": "Physical-AI-Humanoid-Robotics-book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "Physical-AI-Humanoid-Robotics-book/build"
      }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/backend/api.py" },
    { "src": "/(.*)", "dest": "/$1" }
  ]
}